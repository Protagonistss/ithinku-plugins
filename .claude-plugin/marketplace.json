{
  "$schema": "https://anthropic.com/claude-code/marketplace.schema.json",
  "name": "ithinku-plugins",
  "description": "A collection of Claude plugins for development productivity",
  "owner": {
    "name": "huangshan",
    "email": "protagonisths@gmail.com"
  },
  "plugins": [
    {
      "name": "code-review",
      "description": "专业的代码审查插件，提供全面的代码质量分析、安全检查和性能优化建议",
      "source": "./plugins/code-review",
      "version": "0.0.1",
      "keywords": ["code-review", "code-quality", "security", "performance", "best-practices"],
      "category": "development-tools"
    },
    {
      "name": "react-coder",
      "description": "专业的React代码生成工具，提供组件生成、代码优化和最佳实践建议",
      "source": "./plugins/react-coder",
      "version": "0.0.1",
      "keywords": ["react", "code-generation", "best-practices", "hooks"],
      "category": "development-tools"
    },
    {
      "name": "git-tools",
      "description": "智能Git工具集，提供自动化提交、分支管理和Git工作流优化",
      "source": "./plugins/git-tools",
      "version": "0.0.1",
      "keywords": ["git", "version-control", "tools", "commit", "branch", "workflow", "提交", "推送", "拉取", "合并", "克隆", "暂存", "回滚", "撤销", "标签", "版本", "checkout", "merge", "rebase", "stash", "reset", "revert", "cherry-pick", "fetch", "diff", "log", "status", "push", "pull", "add"],
      "category": "development-tools"
    },
    {
      "name": "test-generator",
      "description": "专业的单元测试生成工具，支持多种编程语言和测试框架，能够智能分析代码并生成高质量的测试用例",
      "source": "./plugins/test-generator",
      "version": "0.0.1",
      "keywords": ["testing", "unit-test", "code-generation", "jest", "vitest", "pytest", "junit", "mock", "coverage"],
      "category": "testing"
    },
    {
      "name": "code-polisher",
      "description": "在完全保留所有功能的前提下，对代码进行简化与优化，提升清晰度、一致性和可维护性",
      "source": "./plugins/code-polisher",
      "version": "0.0.1",
      "keywords": ["code-quality", "refactoring", "formatting", "best-practices", "optimization", "代码打磨", "代码优化", "格式化"],
      "category": "development-tools"
    },
    {
      "name": "ui-design",
      "description": "拒绝平庸。专注于生成具有独特审美、Bold Direction 和生产级质量的前端界面",
      "author": {
        "name": "huangshan",
        "email": "protagonisths@gmail.com"
      },
      "source": "./plugins/ui-design",
      "category": "development",
      "homepage": "https://github.com/Protagonistss/claude-plugins/tree/main/plugins/ui-design"
    }
  ]
}